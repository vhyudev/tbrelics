    <style type="text/css">
        #relicList{
            width: 98%;
            padding: 10px;

        }
        #nullData{
            width: 276px;
            height: 154px;
            margin-top: 90px;
            margin-left: 38%;
        }
        .imgItem{
            width: 180px;
            height: 90px;
            float: left;
            margin-right: 23px;
            margin-bottom: 10px;
        }
        .relic-data{
            margin-bottom: 5px;
        }

        .imgBox {
            width: 90px;
            height: 90px;
            margin-right: 10px;
            background-color: #e6e5e5;
            position: relative;
        }

        .imgBox img {
            max-width: 100%;
            max-height: 100%;
            position: absolute;
            top: 0;
            left: 0;
            bottom: 0;
            right: 0;
            margin: auto;
        }

        .itemCtr {
            display: inline-block;
            width: 32px;
            height: 32px;
            text-align: center;
            line-height: 32px;
            background: rgba(0, 0, 0, 0.5);
            border-radius: 2px;
            margin: 0 4px 4px 0;
            cursor: pointer;
            -webkit-user-select: none;
            -moz-user-select: none;
            -ms-user-select: none;
            user-select: none;
        }

    </style>
</head>
<body class="childrenBody">
    <form class="layui-form">
        <blockquote class="layui-elem-quote news_search">
